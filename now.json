  
{
  "version": 2,
  "builds": [{ "src": "app.js", "use": "@now/node-server" }],
  "routes": [
    {
      "src": "/api/github/:username",
      "dest": "/app.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/gitlab/:username",
      "dest": "/app.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/bitbucket/:username",
      "dest": "/app.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/facebook/:username",
      "dest": "/app.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/instagram/:username",
      "dest": "/app.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/twitter/:username",
      "dest": "/app.js",
      "methods": ["GET"]
    }
  ]
}
